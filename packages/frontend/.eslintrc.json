{
  "extends": ["../../.eslintrc.json", "plugin:react/recommended"],
  "env": {
    "browser": true
  },
  "plugins": ["react"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "react/prop-types": "off",
    "@typescript-eslint/no-var-requires": "warn"
  }
}
